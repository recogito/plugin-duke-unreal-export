{
  "name": "@recogito/plugin-duke-unreal-export",
  "version": "0.7.1",
  "description": "A custom annotation export for Duke University's Unreal 3D import",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prepare": "npm run build",
    "dev": "npm start --prefix .dev/",
    "build": "tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"src/**/*.css\" \"src/**/*.jpg\" dist"
  },
  "exports": {
    ".": "./dist/index.js",
    "./thumbnail.jpg": "./dist/assets/teapot_3d.jpg",
    "./DocumentMenuExtension": "./dist/components/DocumentMenuExtension.js",
    "./UnrealExportRoute.ts": "./dist/api/UnrealExportRoute.js"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "copyfiles": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.1.7",
    "@recogito/studio-sdk": "^0.5.0"
  }
}